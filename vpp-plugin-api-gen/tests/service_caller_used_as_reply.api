option version = "1.0.0";

autoendian define create {
    u32 client_index;
    u32 context;
};

autoendian define delete {
    u32 client_index;
    u32 context;
};

autoendian define delete_reply {
    u32 context;
    i32 retval;
};

service {
    rpc create returns delete;
    rpc delete returns delete_reply;
};
